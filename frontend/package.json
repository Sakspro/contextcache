{
    "name": "contextcache-frontend",
    "version": "0.1.0",
    "private": true,
    "description": "Privacy-first memory engine for AI - Frontend",
    "author": "ContextCache Contributors",
    "license": "SEE LICENSE IN ../LICENSING.md",
    "scripts": {
      "dev": "next dev",
      "build": "next build",
      "start": "next start",
      "lint": "next lint",
      "lint:fix": "next lint --fix",
      "format": "prettier --write .",
      "format:check": "prettier --check .",
      "typecheck": "tsc --noEmit",
      "test": "vitest",
      "test:coverage": "vitest --coverage",
      "test:ui": "vitest --ui",
      "test:e2e": "playwright test",
      "test:e2e:ui": "playwright test --ui",
      "test:e2e:debug": "playwright test --debug",
      "clean": "rm -rf .next out node_modules/.cache"
    },
    "dependencies": {
      "next": "^15.1.3",
      "react": "^19.0.0",
      "react-dom": "^19.0.0",
      "zustand": "^5.0.2",
