      "cytoscape-cose-bilkent": "^4.1.0",
      "@tanstack/react-query": "^5.62.7",
      "axios": "^1.7.9",
      "date-fns": "^4.1.0",
      "clsx": "^2.1.1",
      "tailwind-merge": "^2.5.5",
      "lucide-react": "^0.468.0",
      "sonner": "^1.7.1",
      "zod": "^3.24.1",
      "react-hook-form": "^7.54.2",
      "@hookform/resolvers": "^3.9.1"
    },
    "devDependencies": {
      "@types/node": "^22.10.2",
